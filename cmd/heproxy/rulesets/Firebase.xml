<!--
	Nonfunctional subdomains:

		- status

-->
<ruleset name="Firebase (partial)">

	<target host="firebase.com" />
	<target host="*.firebase.com" />


	<securecookie host="^auth\.firebase\.com$" name=".+" />


	<rule from="^http://(auth\.|www\.)?firebase\.com/"
		to="https://$1firebase.com/" />

</ruleset>