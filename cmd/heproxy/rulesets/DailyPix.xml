<ruleset name="DailyPix">

	<target host="dailypix.me" />
	<target host="*.dailypix.me" />
	<target host="lolzparade.com" />
	<target host="*.lolzparade.com" />


	<securecookie host="^(?:w*\.)?(?:dailypix\.me|lolzparade\.com)$" name=".+" />


	<rule from="^http://(www\.)?(dailypix\.me|lolzparade\.com)/"
		to="https://$1$2/" />

</ruleset>