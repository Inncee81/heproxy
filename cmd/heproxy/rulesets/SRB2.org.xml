<!--
	Fully covered subdomains:

		- (www.)
		- files
		- mb
		- ms
		- wiki


	Observed cookie domains:

		- ^
		- .mb
		- wiki

-->
<ruleset name="SRB2.org">

	<target host="srb2.org" />
	<target host="*.srb2.org" />


	<securecookie host="^(?:.+\.)?srb2\.org$" name=".+" />


	<rule from="^http://((?:files|mb|ms|wiki|www)\.)?srb2\.org/"
		to="https://$1srb2.org/" />

</ruleset>
