<!--
	Fully covered subdomains:

		- (www.)
		- lists
		- opensource

-->
<ruleset name="Security Research Labs">

	<target host="srlabs.de" />
	<target host="*.srlabs.de" />


	<securecookie host="^.*\.srlabs\.de$" name=".*" />


	<rule from="^http://((?:lists|opensource|www)\.)?srlabs\.de/"
		to="https://$1srlabs.de/" />

</ruleset>
