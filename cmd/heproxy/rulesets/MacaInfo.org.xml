<!--
	Problematic subdomains:

		- ^	(times out)

-->
<ruleset name="MacaInfo.org">

	<target host="macainfo.org" />
	<target host="*.macainfo.org" />


	<securecookie host="^\.macainfo\.org$" name=".+" />


	<rule from="^http://(?:www\.)?macainfo\.org/"
		to="https://www.macainfo.org/" />

</ruleset>