<!--
	Problematic subdomains:

		- ^	(mismatched, CN: www.web-stat.com)

-->
<ruleset name="Compteur.fr">

	<target host="compteur.fr" />
	<target host="*.compteur.fr" />


	<securecookie host="^\.(?:www\.)?compteur\.fr$" name=".+" />


	<rule from="^http://(?:www\.)?compteur\.fr/"
		to="https://www.compteur.fr/" />

</ruleset>