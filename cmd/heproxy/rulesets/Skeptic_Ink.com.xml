<!--
	Problematic subdomains:

		- ^	(http reply)

-->
<ruleset name="Skeptic Ink.com">

	<target host="skepticink.com" />
	<target host="*.skepticink.com" />


	<securecookie host="^\.skepticink\.com$" name=".+" />


	<rule from="^http://(?:www\.)?skepticink\.com/"
		to="https://www.skepticink.com/" />

</ruleset>