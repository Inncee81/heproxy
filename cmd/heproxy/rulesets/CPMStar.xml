<ruleset name="CPMStar">

	<target host="cpmstar.com" />
	<target host="*.cpmstar.com" />
	<target host="www.server.cpmstar.com" />


	<rule from="^http://(www\.)?(server\.)?cpmstar\.com/"
		to="https://$1$2cpmstar.com/" />

	<rule from="^http://cdn\.cpmstar\.com/"
		to="https://server.cpmstar.com/" />

</ruleset>
