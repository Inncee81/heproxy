<ruleset name="mySecureConnect">

	<target host="mysecureconnect.com" />
	<target host="*.mysecureconnect.com" />


	<securecookie host="^(?:order\.|www\.)?mysecureconnect\.com$" name=".+" />


	<rule from="^http://(order\.|www\.)?mysecureconnect\.com/"
		to="https://$1mysecureconnect.com/" />

</ruleset>