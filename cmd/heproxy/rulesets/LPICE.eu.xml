<!--
	Linux Professional Institute Central Europe


	Fully covered subdomains:

		- (www.)
		- lpievent

-->
<ruleset name="LPICE.eu" platform="cacert">

	<target host="lpice.eu" />
	<target host="*.lpice.eu" />


	<securecookie host="^www\.lpice\.eu$" name=".+" />


	<rule from="^http://(lpievent\.|www\.)?lpice\.eu/"
		to="https://$1lpice.eu/" />

</ruleset>