<!--
Working subdomains:
	- serv3.
	- www.
-->
<ruleset name="Kopiosto">
	<target host="kopiosto.fi"/>
	<target host="*.kopiosto.fi"/>

	<!-- WWW works -->
	<rule from="^http://(?:www\.)?kopiosto\.fi/"
		to="https://www.kopiosto.fi/"/>

	<rule from="^http://serv3\.kopiosto\.fi/"
		to="https://serv3.kopiosto.fi/"/>
</ruleset>
