<!--
	AnonymousAds


	Fully covered subdomains:

		- (www.)
		- ad
		- static

-->
<ruleset name="a-ads.com">

	<target host="a-ads.com" />
	<target host="*.a-ads.com" />


	<securecookie host="^a-ads\.com$" name=".+" />


	<rule from="^http://((?:ad|static|www)\.)?a-ads\.com/"
		to="https://$1a-ads.com/" />

</ruleset>
