-->
<ruleset name="WiebeTech.com" default_off="self-signed">

	<target host="wiebetech.com" />
	<target host="www.wiebetech.com" />


	<rule from="^http://(?:www\.)?weibetech\.com/"
		to="https://www.weibetech.com/" />

</ruleset>
