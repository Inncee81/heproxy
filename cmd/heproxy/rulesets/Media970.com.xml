<ruleset name="media970.com">

	<target host="media970.com" />
	<target host="*.media970.com" />


	<securecookie host="^\.?www\.media970\.com$" name=".+" />


	<rule from="^http://(?:www\.)?media970\.com/"
		to="https://www.media970.com/" />

</ruleset>