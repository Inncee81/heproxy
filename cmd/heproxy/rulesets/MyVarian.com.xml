<!--
	Problematic subdomains:

		- www	(mismatched, CN: acquia-sites.com)

-->
<ruleset name="MyVarian.com">

	<target host="myvarian.com" />
	<target host="*.myvarian.com" />


	<securecookie host="^\.myvarian\.com$" name=".+" />


	<rule from="^http://(?:www\.)?myvarian\.com/"
		to="https://myvarian.com/" />

</ruleset>