<!--
	Fully covered subdomains:

		- (www.)
		- secure
-->
<ruleset name="AccountSupport.com">

	<target host="accountsupport.com" />
	<target host="*.accountsupport.com" />


	<securecookie host="^\.accountsupport\.com$" name=".+" />


	<rule from="^http://(secure\.|www\.)?accountsupport\.com/"
		to="https://$1accountsupport.com/" />

</ruleset>