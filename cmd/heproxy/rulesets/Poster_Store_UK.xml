<!--
	www.../.+ redirects to $

-->
<ruleset name="Poster Store UK">

	<target host="posterstoreuk.com" />
	<target host="*.posterstoreuk.com" />


	<securecookie host="^\.(?:www\.)?posterstoreuk\.com$" name=".+" />


	<rule from="^http://(?:www\.)?posterstoreuk\.com/"
		to="https://posterstoreuk.com/" />

</ruleset>