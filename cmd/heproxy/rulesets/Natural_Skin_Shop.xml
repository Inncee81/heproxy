<ruleset name="Natural Skin Shop">

	<target host="naturalskinshop.com" />
	<target host="*.naturalskinshop.com" />


	<!--	Cookie domains:

			- .
			- .store
			- .www
				-->
	<securecookie host="^.*\.naturalskinshop\.com$" name=".+" />


	<rule from="^http://(store\.|www\.)?naturalskinshop\.com/"
		to="https://$1naturalskinshop.com/" />

</ruleset>