<ruleset name="Simple Pickup (partial)">

	<target host="simplepickup.com" />
	<target host="*.simplepickup.com" />


	<securecookie host="^(?:w*\.)?simplepickup\.com$" name=".+" />


	<rule from="^http://(www\.)?simplepickup\.com/"
		to="https://$1simplepickup.com/" />

</ruleset>