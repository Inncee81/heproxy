<!--
	Problematic domains:

		- ^	(redirects to http)

-->
<ruleset name="Watershed">

	<target host="watershed.co.uk" />
	<target host="*.watershed.co.uk" />


	<rule from="^http://(?:www\.)?watershed\.co\.uk/"
		to="https://www.watershed.co.uk/" />

	<rule from="^http://piwik\.watershed\.co\.uk/"
		to="https://piwik.watershed.co.uk/" />

</ruleset>