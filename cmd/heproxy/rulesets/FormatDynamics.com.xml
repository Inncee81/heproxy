<ruleset name="FormatDynamics.com">

	<target host="formatdynamics.com" />
	<target host="*.formatdynamics.com" />


	<securecookie host="^\.formatdynamics\.com$" name=".+" />


	<rule from="^http://(www\.)?formatdynamics\.com/"
		to="https://$1formatdynamics.com/" />

</ruleset>