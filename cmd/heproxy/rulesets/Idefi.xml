<!--
	CDN buckets:

		- production-idefi.s3.amazonaws.com

-->
<ruleset name="idefi">

	<target host="idefimusic.com" />
	<target host="*.idefimusic.com" />


	<securecookie host="^(?:www)?\.idefimusic.com$" name=".+" />


	<rule from="^http://(www\.)?idefimusic\.com/"
		to="https://$1idefimusic.com/" />

</ruleset>