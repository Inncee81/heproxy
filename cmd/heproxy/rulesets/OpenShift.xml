<ruleset name="OpenShift">

	<target host="openshift.com" />
	<target host="*.openshift.com" />


	<securecookie host="^\.openshift\.com$" name=".+" />


	<rule from="^http://(www\.)?openshift\.com/"
		to="https://$1openshift.com/" />

</ruleset>