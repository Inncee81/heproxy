<ruleset name="Jezebel.com" default_off="mismatched">

	<target host="jezebel.com" />
	<target host="*.jezebel.com" />


	<securecookie host="^\.?jezebel\.com$" name=".+" />


	<rule from="^http://(?:www\.)?jezebel\.com/"
		to="https://jezebel.com/" />

</ruleset>