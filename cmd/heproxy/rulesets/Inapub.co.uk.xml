<ruleset name="Inapub.co.uk">

	<target host="inapub.co.uk" />
	<target host="*.inapub.co.uk" />


	<securecookie host="^(?:w*\.)?inapub\.co\.uk$" name=".+" />


	<rule from="^http://(www\.)?inapub\.co\.uk/"
		to="https://$1inapub.co.uk/" />

</ruleset>