<ruleset name="Host.co.id">

	<target host="host.co.id" />
	<target host="*.host.co.id" />
	<target host="site.biz.id" />
	<target host="*.site.biz.id" />


	<securecookie host="^w*\.(?:site\.biz|host\.co)\.id$" name=".+" />


	<rule from="^http://(www\.)?(site\.biz|host\.co)\.id/"
		to="https://$1$2.id/" />

</ruleset>