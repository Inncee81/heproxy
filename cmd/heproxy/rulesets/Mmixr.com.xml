<!--
	Problematic subdomains:

		- ^	(shows app)

-->
<ruleset name="mmixr.com">

	<target host="mmixr.com" />
	<target host="*.mmixr.com" />


	<securecookie host="^\.mmixr\.com$" name=".+" />


	<rule from="^http://(?:www\.)?mmixr\.com/"
		to="https://www.mmixr.com/" />

	<rule from="^http://app\.mmixr\.com/"
		to="https://app.mmixr.com/" />

</ruleset>