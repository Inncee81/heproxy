<!--
	Fully covered subdomains:

		- (www.)
		- bugs
		- build
		- developer
		- download
		- lists
		- review
		- source
		- wiki

-->
<ruleset name="Tizen.org">

	<target host="tizen.org" />
	<target host="*.tizen.org" />


	<securecookie host="^(?:bugs|build|wiki)\.tizen\.org$" name=".+" />


	<rule from="^http://((?:bugs|build|developer|download|lists|review|source|wiki|www)\.)?tizen\.org/"
		to="https://$1tizen.org/" />

</ruleset>