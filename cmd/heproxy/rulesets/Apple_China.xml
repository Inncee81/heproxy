<!--
	For other Apple coverage, see Apple.xml.


	- !www: no https
	- www:	akamai

-->
<ruleset name="Apple China" default_off="mismatched">

	<target host="apple.com.cn" />
	<target host="www.apple.com.cn" />


	<rule from="^http://(?:www\.)?apple\.com\.cn/"
		to="https://www.apple.com.cn/" />

</ruleset>