<ruleset name="Washington Free Beacon (problematic)" default_off="self-signed">

	<target host="freebeacon.com" />
	<target host="www.freebeacon.com" />


	<rule from="^http://(?:www\.)?freebeacon\.com/"
		to="https://freebeacon.com/" />

</ruleset>