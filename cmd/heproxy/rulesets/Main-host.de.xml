<!--
	Problematic subdomains:

		- ^	(404, valid cert)

-->
<ruleset name="main-host.de">

	<target host="main-host.de" />
	<target host="*.main-host.de" />


	<rule from="^http://(?:www\.)?main-host\.de/"
		to="https://www.main-host.de/" />

	<rule from="^http://kunden\.main-host\.de/"
		to="https://kunden.main-host.de/" />

</ruleset>
