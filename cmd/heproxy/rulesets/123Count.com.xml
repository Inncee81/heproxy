<!--
	Problematic subdomains:

		- ^	(mismatched, CN: www.web-stat.com)

-->
<ruleset name="123Count.com">

	<target host="123count.com" />
	<target host="*.123count.com" />


	<securecookie host="^\.123count\.com$" name=".+" />


	<rule from="^http://(?:www\.)?123count\.com/"
		to="https://www.123count.com/" />

</ruleset>