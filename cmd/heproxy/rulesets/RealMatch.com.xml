<ruleset name="RealMatch.com (partial)">

	<target host="realmatch.com" />
	<target host="*.realmatch.com" />


	<securecookie host="^\.realmatch\.com$" name="^__utm\w$" />
	<securecookie host="^www\.realmatch\.com$" name=".+" />


	<rule from="^http://(www\.)?realmatch\.com/"
		to="https://$1realmatch.com/" />

</ruleset>