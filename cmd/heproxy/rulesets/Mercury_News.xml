<ruleset name="Mercury News">

	<target host="mmedia.me" />
	<target host="*.mmedia.me" />


	<!--	Observed cookies domains:

			- ^
			- .
			- now
					-->
	<securecookie host="^(?:.*\.)?mmedia\.me$" name=".+" />


	<rule from="^http://(now\.|rss\.|www\.)?mmedia\.me/"
		to="https://$1mmedia.me/" />

</ruleset>