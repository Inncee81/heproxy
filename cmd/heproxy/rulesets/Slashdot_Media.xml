<!--
	For other Dice coverage, see Dice.xml.


	Fully covered subdomains:

		- (www.)
		- library
		- web

-->
<ruleset name="Slashdot Media">

	<target host="slashdotmedia.com" />
	<target host="*.slashdotmedia.com" />


	<rule from="^http://((?:library|web|www)\.)?slashdotmedia\.com/"
		to="https://$1slashdotmedia.com/" />

</ruleset>